{
  "name": "prettier-plugin-csharpier",
  "version": "0.1.0-development",
  "description": "Prettier C# Plugin",
  "repository": "belav/csharpier",
  "author": "Bela VanderVoort",
  "license": "MIT",
  "main": "dist/prettier-plugin-csharpier",
  "dependencies": {
    "prettier": "^2.1.2"
  },
  "scripts": {
    "start": "webpack --config=config/development.config.js",
    "build": "webpack --config=config/production.config.js",
    "prettier": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "test": "jest",
    "samples": "node ./Samples/_index.js",
    "csharpier": "prettier --plugin=. --parser=cs",
    "csharpier:debug": "node --lazy --inspect-brk src/debug.js"
  },
  "nodemonConfig": {
    "watch": "src/Printer/Types",
    "exec": "webpack --config=config/development.config.js"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@types/prettier": "2.1.5",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.2.1",
    "copy-webpack-plugin": "6.3.1",
    "jest": "26.6.3",
    "plop": "2.7.4",
    "source-map-loader": "1.1.2",
    "typescript": "4.0.5",
    "webpack": "5.19.0",
    "webpack-cli": "4.2.0",
    "webpack-merge": "4.2.2"
  }
}
